{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-danger": "off",
    "class-methods-use-this": "off",
    "react/react-in-jsx-scope": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/jsx-no-bind": 0
  },
  "globals": {
    "fetch": true
  },
  "plugins": [
    "jest"
  ]
}